		"@babel/preset-typescript": "^7.10.1",
		"@babel/register": "^7.10.1",
		"@babel/runtime": "^7.10.2",
		"@types/enzyme": "^3.10.5",
		"@types/jest": "^25.2.3",
		"@types/react": "^16.9.35",
		"@types/react-dom": "^16.9.8",
