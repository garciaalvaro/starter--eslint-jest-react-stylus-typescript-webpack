{
	"name": "starter--eslint-jest-react-stylus-typescript-webpack",
	"version": "1.0.0",
	"description": "Starter project with the minimum configuration to begin development",
	"scripts": {
		"webpack:dev": "webpack --mode development --watch",
		"webpack:prod": "webpack --mode production",
		"start": "npm run webpack:dev",
		"preprod": "npm run test",
		"prod": "npm run webpack:prod",
		"test": "npm run type-check && npm run lint && npm run prettier && jest",
		"type-check": "tsc",
		"jest": "jest --watch",
		"lint": "eslint 'src/**/*.{ts,tsx}'",
		"prettier": "prettier --check 'src/**/*.{ts,tsx}'"
	},
	"dependencies": {
		"react": "^16.12.0",
		"react-dom": "^16.12.0"
	},
